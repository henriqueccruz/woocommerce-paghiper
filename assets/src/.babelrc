{
    "presets": [
        "@babel/preset-react",
        ["@babel/preset-env", {
            "targets": {
                "browsers": [
                    "last 2 versions",
                    "ie >= 11"
                ]
            }
        }]
    ],
    "plugins": [
        ["@babel/plugin-transform-react-jsx", {
            "pragma": "wp.element.createElement",
            "pragmaFrag": "wp.element.Fragment"
        }]
    ]
}